class Game{
  int size;
  int nbrVehicles;
  Vehicle[] vehicles;
  State etat_initial;
  
  LinkedList<Move> solve();
}

class Vehicle{
	int id;
	int length;
	char orientation;
	int fixedPos;
}

class Move{
	int id;
	int distance;

class State{
  	int[] pos;
  	boolean[][] is_occupied;
  
  	LinkedList<Move> possible_moves();
 	Etat nextState(move);
	Move getLastMove(SeenStates T);
  	void print();
}

class SeenStates{
	Move lastmove;
	SeenStates[] children;
	
	boolean add(State e, Move lastmove);
}
